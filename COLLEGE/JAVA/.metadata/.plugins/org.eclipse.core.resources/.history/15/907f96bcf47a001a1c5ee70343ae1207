import javax.swing.*;


public class mySwingProgram {

	public static void main(String[] args) {
		
		JFrame jf = new JFrame("My Frame");
		
		JLabel jl = new JLabel("User Name");
		jl.setBounds(50,100,70,30);
		
		JTextField jtf = new JTextField();
		jtf.setBounds(130,100,100,30);
		jtf.add(jl);
		
		JLabel jp = new JLabel("Password");
		jp.setBounds(50,140,70,30);
		jtf.add(jp);
		
	}

}
