import java.applet.*;
import java.awt.*; // abstract windows toolkit

public class myApplet extends Applet{

	String msg = "Applet demo";
	public void init()
	{
		setBackground(Color.blue);
	}
	
	public void start()
	{
		
	}
	
	public void paint(Graphics g)
	{
		g.drawString(msg,50,100);
	}
	
	public void stop()
	{
		
	}
}
