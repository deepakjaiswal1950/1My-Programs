class A{
	void show(){
		System.out.println("Base class");
	}
}

class B extends A{
	void show(){ // method overriding
		super.show(); // To access base class method
		System.out.println("Sub class");
	}
}
public class Main {

	public static void main(String[] args) {
		
		B obj = new B();
		obj.show();

	}

}
